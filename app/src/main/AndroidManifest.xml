<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.babulgam.facebookdemologinsysytem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/facebook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/facebook"
        android:supportsRtl="true"
        android:theme="@style/Theme.FacebookDemoLoginSysytem">
        <activity android:name=".Forget_password"
            android:parentActivityName=".Login_Activity"
            ></activity>
        <activity
            android:name=".profile_activity"
            android:parentActivityName=".Login_Activity" />
        <activity
            android:name=".Signup_layout"
            android:parentActivityName=".Login_Activity" />
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>